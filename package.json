{
  "name": "express-typescript-starter",
  "version": "0.1.0",
  "description": "A starting point for Node.js express apps with TypeScript",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/TypeScript-Node-Starter"
  },
  "author": "Bowden Kelly",
  "license": "MIT",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "start": "npm run serve",
    "build": "npm run build-sass && npm run build-ts && npm run lint && npm run copy-static-assets",
    "serve": "node dist/server.js",
    "watch-node": "nodemon dist/server.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-sass": "node-sass src/public/css/main.scss dist/public/css/main.css",
    "watch-sass": "node-sass -w src/public/css/main.scss dist/public/css/main.css",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "copy-static-assets": "ts-node copyStaticAssets.ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/server.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "migration:init": "npx sequelize init:config && npx sequelize init:migrations",
    "migration:create": "run(){ NODE_ENV=local npx sequelize migration:generate --name=$1; }; run",
    "migration:run": "NODE_ENV=local npx sequelize db:migrate"
  },
  "dependencies": {
    "@types/cors": "^2.8.6",
    "apollo-server-express": "^2.11.0",
    "async": "^3.1.0",
    "bcrypt-nodejs": "^0.0.3",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-mongo": "^3.0.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "express-session": "^1.16.2",
    "express-validator": "^6.2.0",
    "fbgraph": "^1.4.4",
    "graphql": "^14.6.0",
    "graphql-tools": "^4.0.7",
    "http": "0.0.1-security",
    "lodash": "^4.17.15",
    "lusca": "^1.6.1",
    "mysql2": "^2.1.0",
    "ncp": "^2.0.0",
    "nodemailer": "^6.3.0",
    "passport": "^0.4.0",
    "passport-facebook": "^3.0.0",
    "passport-local": "^1.0.0",
    "pug": "^2.0.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.21.5",
    "sequelize-typescript": "^1.1.0",
    "sqlite": "^3.0.6",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/async": "^3.0.2",
    "@types/bcrypt-nodejs": "^0.0.30",
    "@types/bluebird": "^3.5.30",
    "@types/body-parser": "^1.17.1",
    "@types/chai": "^4.2.3",
    "@types/compression": "^1.7.0",
    "@types/concurrently": "^4.1.0",
    "@types/connect-mongo": "^3.0.0",
    "@types/errorhandler": "^0.0.32",
    "@types/eslint": "^6.1.1",
    "@types/express": "^4.17.4",
    "@types/express-session": "^1.15.14",
    "@types/graphql": "^14.5.0",
    "@types/graphql-depth-limit": "^1.1.2",
    "@types/jest": "^24.0.23",
    "@types/jquery": "^3.3.31",
    "@types/lodash": "^4.14.141",
    "@types/lusca": "^1.6.1",
    "@types/mongoose": "^5.5.18",
    "@types/node": "^12.12.34",
    "@types/node-sass": "^4.11.0",
    "@types/nodemailer": "^6.2.1",
    "@types/passport": "^1.0.1",
    "@types/passport-facebook": "^2.1.9",
    "@types/passport-local": "^1.0.33",
    "@types/pug": "^2.0.4",
    "@types/request": "^2.48.3",
    "@types/request-promise": "^4.1.44",
    "@types/sequelize": "^4.28.8",
    "@types/shelljs": "^0.8.5",
    "@types/supertest": "^2.0.8",
    "@types/validator": "^12.0.1",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^2.3.1",
    "@typescript-eslint/parser": "^2.3.1",
    "chai": "^4.2.0",
    "concurrently": "^5.0.0",
    "eslint": "^6.4.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-import": "^1.0.1",
    "graphql-import-node": "0.0.4",
    "jest": "^24.9.0",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.2",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.3"
  }
}
